{
  "name": "post-it",
  "version": "1.0.0",
  "description": "Post It app for posting messages to different groups",
  "main": "index.js",
  "engines": {
    "node": "6.10.0",
    "npm": "3.10.10"
  },
  "scripts": {
    "test": "NODE_ENV=test mocha  server/test  --require babel-polyfill --compilers js:babel-register",
    "build": "babel server -d dist --presets es2015,stage-2",
    "start": "babel-node server/app.js --presets es2015,stage-2",
    "serve": "node dist/app.js",
    "server": "nodemon server/app.js --exec babel-node --presets es2015,stage-2",
    "eslint": "./node_modules/.bin/eslint server/app.js",
    "coveralls": "istanbul cover ./node_modules/mocha/bin/_mocha dist/test --report lcovonly && cat ./coverage/lcov.info | coveralls",
    "delete-dist": "rm -r dist",
    "enter-server": "cd server",
    "undo:tests": "NODE_ENV=test sequelize db:migrate:undo:all",
    "db:migrate": "NODE_ENV=test sequelize db:migrate",
    "test-integration": "cross-env NODE_ENV=development mocha server/test/integration/*.js --require babel-polyfill --compilers js:babel-register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mikey2020/post-it.git"
  },
  "author": "Michael Eboagu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mikey2020/post-it/issues"
  },
  "homepage": "https://github.com/mikey2020/post-it#readme",
  "devDependencies": {
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-es2015-parameters": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "gulp": "^3.9.1",
    "gulp-coveralls": "^0.1.4",
    "gulp-istanbul": "^1.1.2",
    "gulp-mocha": "^4.3.1",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "dependencies": {
    "axios": "0.12.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "crypto": "0.0.3",
    "css-loader": "^0.28.4",
    "debug": "^2.6.8",
    "dotenv": "^4.0.0",
    "express": "^4.15.3",
    "express-session": "^1.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "gulp-babel": "^6.1.2",
    "lodash": "^4.17.4",
    "method-override": "^2.3.9",
    "morgan": "^1.8.2",
    "nodemon": "^1.11.0",
    "pg": "^6.2.4",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.5.10",
    "sass-loader": "^6.0.6",
    "sequelize": "^4.0.0",
    "sequelize-cli": "^2.7.0",
    "shortid": "^2.2.8",
    "should": "^11.2.1",
    "style-loader": "^0.18.2",
    "superagent": "^3.5.2",
    "supertest": "^3.0.0",
    "validator": "^7.0.0"
  }
}
